<PropertyList>

	<path>PA28-161.ac</path>

	<!-- LIVERY SELECT -->
	<!-- Animation -->
	<animation>
		<type>material</type>
		<!-- Fuselage -->
		<object-name>Fuselage</object-name>
		<object-name>VTail</object-name>
		<object-name>Rudder</object-name>
		<object-name>Door</object-name>
		<object-name>GearSub</object-name>
		<object-name>Spinner</object-name>
		<object-name>doorF</object-name>
		<object-name>doorB</object-name>
		<property-base>sim/model/livery</property-base>
		<texture-prop>texture</texture-prop>
		<texture>PA28-WarriorII.png</texture>
	</animation>
	
	<!-- Nasal code -->
	<nasal>
		<load>
			var livery_update = aircraft.livery_update.new("Aircraft/PA28/Models/Liveries");
			
			var rplayer = cmdarg();
			
			rplayer.getNode("fdm/jsbsim/crash-stress/nose-damaged",		1).alias(rplayer.getNode("sim/multiplay/generic/int[0]"));
			rplayer.getNode("fdm/jsbsim/crash-stress/lwing-damaged",	1).alias(rplayer.getNode("sim/multiplay/generic/int[1]"));
			rplayer.getNode("fdm/jsbsim/crash-stress/rwing-damaged",	1).alias(rplayer.getNode("sim/multiplay/generic/int[2]"));
			rplayer.getNode("fdm/jsbsim/crash-stress/htail-damaged",	1).alias(rplayer.getNode("sim/multiplay/generic/int[3]"));
			rplayer.getNode("fdm/jsbsim/crash-stress/vtail-damaged",	1).alias(rplayer.getNode("sim/multiplay/generic/int[4]"));
			rplayer.getNode("controls/lighting/landing-light",		1).alias(rplayer.getNode("sim/multiplay/generic/int[5]"));
			rplayer.getNode("controls/lighting/nav-lights",			1).alias(rplayer.getNode("sim/multiplay/generic/int[6]"));
			rplayer.getNode("sim/model/lights/strobe/state",		1).alias(rplayer.getNode("sim/multiplay/generic/int[7]"));
			rplayer.getNode("sim/model/lights/beacon/state",		1).alias(rplayer.getNode("sim/multiplay/generic/int[8]"));
			
			rplayer.getNode("fdm/jsbsim/fcs/aileron-yoke",			1).alias(rplayer.getNode("sim/multiplay/generic/float[0]"));
			rplayer.getNode("fdm/jsbsim/fcs/pitch-input",			1).alias(rplayer.getNode("sim/multiplay/generic/float[1]"));
			rplayer.getNode("fdm/jsbsim/fcs/yaw-input",			1).alias(rplayer.getNode("sim/multiplay/generic/float[2]"));
			rplayer.getNode("fdm/jsbsim/fcs/steer-deg",			1).alias(rplayer.getNode("sim/multiplay/generic/float[3]"));
			rplayer.getNode("fdm/jsbsim/extra/door-main-norm",		1).alias(rplayer.getNode("sim/multiplay/generic/float[4]"));
			rplayer.getNode("fdm/jsbsim/extra/door-baggage-norm",		1).alias(rplayer.getNode("sim/multiplay/generic/float[5]"));
		</load>
		<unload>
			livery_update.stop();
			
			rplayer.getNode("fdm/jsbsim/crash-stress/nose-damaged",		1).unalias();
			rplayer.getNode("fdm/jsbsim/crash-stress/lwing-damaged",	1).unalias();
			rplayer.getNode("fdm/jsbsim/crash-stress/rwing-damaged",	1).unalias();
			rplayer.getNode("fdm/jsbsim/crash-stress/htail-damaged",	1).unalias();
			rplayer.getNode("fdm/jsbsim/crash-stress/vtail-damaged",	1).unalias();
			rplayer.getNode("controls/lighting/landing-light",		1).unalias();
			rplayer.getNode("controls/lighting/nav-lights",			1).unalias();
			rplayer.getNode("sim/model/lights/strobe/state",		1).unalias();
			rplayer.getNode("sim/model/lights/beacon/state",		1).unalias();
			
			rplayer.getNode("fdm/jsbsim/fcs/aileron-yoke",			1).unalias();
			rplayer.getNode("fdm/jsbsim/fcs/pitch-input",			1).unalias();
			rplayer.getNode("fdm/jsbsim/fcs/yaw-input",			1).unalias();
			rplayer.getNode("fdm/jsbsim/fcs/steer-deg",			1).unalias();
			rplayer.getNode("fdm/jsbsim/extra/door-main-norm",		1).unalias();
			rplayer.getNode("fdm/jsbsim/extra/door-baggage-norm",		1).unalias();
		</unload>
	</nasal>
	
