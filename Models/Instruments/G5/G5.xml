<?xml version="1.0"?>

<PropertyList>
	
	<path>G5.ac</path>
	
	<animation>
		<type>pick</type>
		<object-name>G5.powerbtn</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>nasal</command>
				<script>
					g5.power_btn(1);
				</script>
			</binding>
			<mod-up>
				<binding>
					<command>nasal</command>
					<script>
						g5.power_btn(0);
					</script>
				</binding>
			</mod-up>
		</action>
	</animation>

</PropertyList>
