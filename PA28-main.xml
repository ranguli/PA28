			<default>
				<menu n="3">
					<enabled type="bool">true</enabled>
				</menu>
				<menu n="100">
					<label>|</label>
					<enabled type="bool">false</enabled>
				</menu>
				<menu n="101">
					<label>System</label>
					<enabled type="bool">true</enabled>
					<item>
						<label>Aircraft Config</label>
						<binding>
							<command>nasal</command>
							<script>acconfig.main_dlg.open();</script>
						</binding>
					</item>
					<item>
						<label>About This Aircraft</label>
						<binding>
							<command>nasal</command>
							<script>acconfig.about_dlg.open();</script>
						</binding>
					</item>
				</menu>
				<menu n="102">
					<label>Utilities</label>
					<enabled type="bool">true</enabled>
					<item>
						<label>Select Livery</label>
						<binding>
							<command>nasal</command>
							<script>aircraft.livery.dialog.toggle();</script>
						</binding>
					</item>
					<item>
						<label>Mini Panel</label>
						<binding>
							<command>nasal</command>
							<script>
								acconfig.minipanel_dlg.open();
								setprop("/options/mini-panel", 1);
								acconfig.writeSettings();
							</script>
						</binding>
					</item>
					<item>
						<label>Control Panel</label>
						<binding>
							<command>nasal</command>
							<script>acconfig.controlpanel_dlg.open();</script>
						</binding>
					</item>
					<item>
						<label>Failures</label>
						<binding>
							<command>nasal</command>
							<script>acconfig.fail_dlg.open();</script>
						</binding>
					</item>
					<item>
						<label>Repair Aircraft</label>
						<binding>
							<command>nasal</command>
							<script>
								<![CDATA[
									if (getprop("/position/gear-agl-ft") <= 1) {
										libraries.crashStress.reset();
										acconfig.failReset();
									} else {
										gui.popupTip("How can you repair the aircraft in flight? Please land first! :)");
									}
								]]>
							</script>
						</binding>
					</item>
					<item>
						<label>Flashlight</label>
						<binding>
							<command>property-cycle</command>
							<property>/sim/rendering/als-secondary-lights/use-flashlight</property>
							<value>1</value>
							<value>2</value>
							<value>0</value>
						</binding>
					</item>
				</menu>
				<menu n="103">
					<label>Documentation</label>
					<enabled type="bool">true</enabled>
					<item>
						<label>Fuel Management</label>
						<binding>
							<command>open-browser</command>
							<path>Aircraft/PA28/Docs/FuelManagement.pdf</path>
						</binding>
					</item>
					<item>
						<label>About the Mini Panel</label>
						<binding>
							<command>open-browser</command>
							<path>Aircraft/PA28/Docs/MiniPanel.pdf</path>
						</binding>
					</item>
					<item>
						<label>Optimal Rendering Settings</label>
						<binding>
							<command>open-browser</command>
							<path>Aircraft/PA28/Docs/Rendering.pdf</path>
						</binding>
					</item>
				</menu>
			</default>
