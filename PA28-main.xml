			</key>
			<key n="55">
				<name>7</name>
				<desc>Elevator trim down</desc>
				<binding>
					<command>nasal</command>
					<script>controls.elevatorTrim(1);</script>
				</binding>
				<mod-shift>
					<desc>Look front left</desc>
					<binding>
						<command>property-assign</command>
						<property>/sim/current-view/goal-heading-offset-deg</property>
						<property>/sim/view/config/front-left-direction-deg</property>
					</binding>
				</mod-shift>
			</key>
			<key n="66">
				<name>B</name>
				<desc>Toggle parking brake on or off</desc>
				<binding>
					<command>nasal</command>
					<script>controls.applyParkingBrake(1)</script>
				</binding>
				<mod-up>
					<binding>
						<command>nasal</command>
						<script>controls.applyParkingBrake(0)</script>
					</binding>
				</mod-up>	
			</key>
			<key n="68">
				<name>D</name>
				<desc>Autopilot Disconnect</desc>
				<binding>
					<command>property-assign</command>
					<property>/it-stec55x/input/disc</property>
					<value>1</value>
				</binding>
				<binding>
					<command>nasal</command>
					<script>stec55x.button.DISC();</script>
				</binding>
				<mod-up>
					<binding>
						<command>property-assign</command>
						<property>/it-stec55x/input/disc</property>
						<value>0</value>
					</binding>
				</mod-up>
			</key>
			<key n="88">
				<name>X</name>
				<desc>Increase field of view</desc>
				<binding>
					<command>nasal</command>
					<script>libraries.fovZoom(1);</script>
				</binding>
			</key>
			<key n="99">
				<name>c</name>
				<desc>CWS Button S-TEC 55X / KAP 140</desc>
				<binding>
					<command>nasal</command>
					<script>stec55x.button.CWS(1);</script>
				</binding>
				<binding>
					<command>property-assign</command>
					<property>autopilot/kap140/settings/cws</property>
					<value>1</value>
				</binding>
				<mod-up>
					<binding>
						<command>nasal</command>
						<script>stec55x.button.CWS(0);</script>
					</binding>
					<binding>
						<command>property-assign</command>
						<property>autopilot/kap140/settings/cws</property>
						<value>0</value>
					</binding>
				</mod-up>
			</key>
			<key n="101">
				<name>e</name>
				<desc>Thrust Lever Idle</desc>
				<binding>
					<command>nasal</command>
					<script>libraries.doIdleThrust();</script>
				</binding>
			</key>
			<key n="102">
				<name>f</name>
				<desc>Thrust Lever Full</desc>
				<binding>
					<command>nasal</command>
					<script>libraries.doFullThrust();</script>
				</binding>
			</key>
			<key n="106">
				<name>j</name>
				<desc>toggle tanks</desc>
				<binding>
					<command>property-cycle</command>
					<property>/systems/fuel/selected-tank</property>
					<value>1</value>
					<value>2</value>
				</binding>
			</key>
			<key n="120">
				<name>x</name>
				<desc>Decrease field of view</desc>
				<binding>
					<command>nasal</command>
					<script>libraries.fovZoom(-1);</script>
				</binding>
			</key>
			<key n="362">
				<name>Home</name>
				<desc>Elevator Trim Down</desc>
				<binding>
					<command>nasal</command>
					<script>controls.elevatorTrim(1);</script>
				</binding>
			</key>
			<key n="363">
				<name>End</name>
				<desc>Elevator Trim Up</desc>
				<binding>
					<command>nasal</command>
					<script>controls.elevatorTrim(-1);</script>
				</binding>
			</key>
		</keyboard>
